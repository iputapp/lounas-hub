{
  "name": "lounas-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export",
    "eslint:fix": "eslint ./src --ext .js,jsx,.ts,.tsx --fix",
    "stylelint:fix": "stylelint ./src/**/*.{css,scss} --fix",
    "prettier:fix": "prettier --check --write './src/**/*.{js,jsx,ts,tsx,css,scss,md,mdx}'",
    "format": "npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
    "db:gen": "prisma generate",
    "db:mig:dev": "dotenv -e .env.local prisma migrate dev",
    "db:studio": "dotenv -e .env.local prisma studio"
  },
  "dependencies": {
    "@hookform/resolvers": "^3",
    "@nextui-org/react": "^2",
    "@prisma/client": "^5",
    "@supabase/ssr": "^0",
    "@supabase/supabase-js": "^2",
    "cloudinary": "^1.41.0",
    "framer-motion": "^10",
    "nanoid": "^5.0.3",
    "next": "14.1.1",
    "next-cloudinary": "^5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7",
    "swr": "^2.2.4",
    "tailwindcss": "^3",
    "zod": "^3",
    "zod-prisma-types": "^3"
  },
  "devDependencies": {
    "@svgr/webpack": "^8",
    "@types/node": "^20.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "autoprefixer": "^10",
    "dotenv-cli": "^7",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9",
    "eslint-plugin-import": "^2",
    "eslint-plugin-simple-import-sort": "^10",
    "eslint-plugin-unused-imports": "^3",
    "postcss": "^8",
    "prettier": "^3",
    "prettier-plugin-tailwindcss": "^0",
    "prisma": "^5",
    "sass": "^1",
    "stylelint": "^15",
    "stylelint-config-recess-order": "^4",
    "stylelint-config-standard": "^34",
    "stylelint-config-standard-scss": "^11",
    "stylelint-scss": "^5",
    "typescript": "^5"
  }
}
